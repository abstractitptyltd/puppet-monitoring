{
    "name": "abstractit-monitoring",
    "version": "1.3.4",
    "author": "Abstract IT",
    "license": "Apache-2.0",
    "summary": "Automagical monitoring with Puppet",
    "source": "https://github.com/abstractitptyltd/puppet-monitoring",
    "project_page": "https://github.com/abstractitptyltd/puppet-monitoring",
